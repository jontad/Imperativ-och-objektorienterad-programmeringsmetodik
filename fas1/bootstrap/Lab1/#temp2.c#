int main(int argc, char *argv[])
{
  if (argc > 1)
    {
  if (is_number(argv[1]) && is_number(argv[2])) {printf("%s and %s are numbers\n", argv[1], argv[2]);}

  else
    {
    
      if (!(is_number(argv[1])) && !(is_number(argv[2])))  {printf("%s and %s are not numbers\n", argv[1], argv[2]);}

      else
	{
	  if (is_number(argv[1]) && !(is_number(argv[2]))) {printf("Only %s is a number\n", argv[1]);}
	  
	  else
	    {
	      if (is_number(argv[2]) && !(is_number(argv[1]))) {printf("Only %s is a number\n", argv[2]);}
		
	      
	    }
	}
    }
    }
  if (arg <= 1=) {printf("Please provide a command line argument!\n");}
  